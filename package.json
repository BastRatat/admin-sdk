{
  "name": "@bastratat/admin-sdk-auth",
  "version": "1.0.0",
  "description": "Unified TypeScript SDK for Supabase Auth verification, RBAC, and admin operations",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "./build.sh",
    "build:cjs": "npx tsc -p tsconfig.cjs.json",
    "build:esm": "npx tsc -p tsconfig.esm.json",
    "dev": "tsc --watch",
    "clean": "rm -rf dist dist-cjs dist-esm",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.38.0",
    "jsonwebtoken": "^9.0.2",
    "jwks-client": "^2.0.5",
    "node-cache": "^5.1.2"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "eslint": "^8.55.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.0"
  },
  "keywords": [
    "supabase",
    "auth",
    "jwt",
    "rbac",
    "admin",
    "typescript",
    "sdk"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/admin-sdk.git"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
